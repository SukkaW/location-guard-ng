{
  "compilerOptions": {
    "incremental": true,
    "target": "es2020",
    "lib": ["ES2022", "DOM", "DOM.Iterable", "DOM.AsyncIterable"],
    "jsx": "react-jsx",
    "module": "preserve",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": false,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
