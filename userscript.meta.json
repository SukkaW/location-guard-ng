{
  "name": "Location Guard",
  "namespace": "https://skk.moe",
  "grant": [
    "unsafeWindow",
    "GM_getValue",
    "GM_setValue",
    "GM.getValue",
    "GM.setValue",
    "GM_deleteValue",
    "GM.deleteValue",
    "GM.registerMenuCommand",
    "GM_addValueChangeListener",
    "GM_removeValueChangeListener",
    "GM.addValueChangeListener",
    "GM.removeValueChangeListener"
  ],
  "run-at": "document-end",
  "match": ["*://*/*"],
  "updateURL": "https://cdn.jsdelivr.net/npm/location-guard@latest/dist/location-guard-ng.meta.js",
  "downloadURL": "https://cdn.jsdelivr.net/npm/location-guard@latest/dist/location-guard-ng.user.js"
}
